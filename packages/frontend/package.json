{
  "name": "@astroparty/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn clean && tsc && cross-env NODE_ENV=production vite build",
    "preview": "vite preview",
    "deploy": "vk-miniapps-deploy",
    "lint": "yarn lint:types && yarn lint:eslint && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix",
    "lint:types": "yarn tsc --noemit",
    "lint:eslint": "yarn eslint .",
    "lint:eslint:fix": "yarn eslint . --fix",
    "lint:prettier": "yarn prettier . -c",
    "lint:prettier:fix": "yarn prettier . -w",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@emotion/css": "^11.10.0",
    "@emotion/is-prop-valid": "^1.2.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@reduxjs/toolkit": "^1.9.2",
    "react-redux": "^8.0.5",
    "@sentry/react": "^7.43.0",
    "@sentry/tracing": "^7.43.0",
    "@vkontakte/api-schema-typescript": "^1.5131.2",
    "@vkontakte/icons": "^2.19.0",
    "@vkontakte/vk-bridge": "^2.7.2",
    "@vkontakte/vkui": "^5.5.0",
    "axios": "^1.4.0",
    "vite-plugin-mkcert": "^1.12.0",
    "vite-plugin-singlefile": "^0.13.1",
    "classnames": "^2.3.2",
    "pixi-viewport": "^5.0.1",
    "pixi.js": "^7.2.4",
    "@astroparty/shared": "*",
    "@astroparty/prisma": "*"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.5",
    "@sentry/vite-plugin": "^0.4.0",
    "@vkontakte/vk-miniapps-deploy": "^0.1.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-storybook": "^0.6.11",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "rimraf": "^5.0.0"
  }
}